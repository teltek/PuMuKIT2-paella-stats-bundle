{
    "name": "teltek/pmk2-paella-stats-bundle",
    "description": "Stats for PuMuKIT and Paella",
    "type": "symfony-bundle",
    "keywords": [
        "stats",
        "symfony2",
        "bundle",
        "pumukit",
        "paella"
    ],
    "require": {
        "php": ">=5.5.9",
        "teltek/pmk2-paella-player-bundle": ">=1.2",
        "pumukit/pumukit2": ">=2.2",
        "gpslab/geoip2": "^1.0"
    },
    "homepage": "https://github.com/teltek/PuMuKIT2-paella-stats-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Teltek Team",
            "homepage": "http://teltek.es"            
        }
    ],
    "autoload": {
        "psr-4": {
            "Pumukit\\PaellaStatsBundle\\": ""
        }
    },
    "scripts": {
        "post-install-cmd": [
            "GpsLab\\Bundle\\GeoIP2Bundle\\Composer\\ScriptHandler::updateDatabase"
        ],
        "post-update-cmd": [
            "GpsLab\\Bundle\\GeoIP2Bundle\\Composer\\ScriptHandler::updateDatabase"
        ]
    }
}
